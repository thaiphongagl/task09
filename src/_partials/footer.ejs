<footer class="c-footer">
    <div class="c-footer__nav">
        <div class="c-footer__logo">
            <a href="./index.html">
                <img src="./assets/img/logo.png" alt="Entex Recruitment">
                <span class="c-footer__logotitle">Recruitment 2020</span>
            </a>
        </div>
        
        <nav class="c-gnav is-pcntab">
            <ul>
                <% if (typeof linklists !='undefined' ) { %>
                    <% linklists.main && linklists.main.forEach(link=> { %>
                        <li <% if(link.class) {%>class="c-footer__gnavitem <%=link.class%>" <%} %> >
                                    <a href="<%= link.url%>">
                                        <%= link.title %>
                                    </a>
                        </li>
                    <% }) %>
                <% } %>
            </ul>
        </nav>

        <a href="#" class="c-footer__assignment is-pcntab">
            <img src="./assets/img/assignment.svg" alt="募集要項">
            <span>募集要項</span>
        </a>
    </div>

    <div class="c-footer__externalsite">
        <a class="c-footer__link" href="#" target="_blank">> 株式会社ENTEXコーポレートサイト</a>
    </div>

    <div class="c-footer__copyright">
        © 株式会社ENTEX. All rights reserved.
    </div>
</footer>